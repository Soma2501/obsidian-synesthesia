{
  "name": "obsidian-synesthesia",
  "version": "0.1.0",
  "description": "Theme",
  "author": "Akash S. Patel",
  "license": "GPL-3.0",
  "scripts": {
    "build": "vite build -c dev-configs/vite.config.ts",
    "postbuild": "node scripts/postbuild.js",
    "buildx": "npm run build && npm run postbuild",
    "watch": "vite --watch",
    "build:temp": "sass src/theme.scss dist/theme.css && postcss dist/theme.css -o dist/theme.css",
    "watch:temp": "sass --watch src/theme.scss dist/theme.css",
    "lint": "eslint \"src/**/*.{ts,js}\" \"dev-configs/**/*.{ts,js}\" -c ./dev-configs/.eslintrc.mjs",
    "lint:css": "stylelint \"**/*.{css,scss}\" -c dev-configs/stylelint.config.js",
    "lint:css:fix": "stylelint \"**/*.{css,scss}\" --fix -c dev-configs/stylelint.config.js",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "security": "npm audit",
    "security:fix": "npm audit fix",
    "check": "npm-run-all lint:css security",
    "unused": "npm-check",
    "prune": "ts-prune -p dev-configs/tsconfig.json"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@types/node": "^22.15.3",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.26.0",
    "eslint-plugin-filenames": "^1.3.2",
    "fs-extra": "^11.3.0",
    "npm-check": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "obsidian": "^1.8.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "sass": "^1.87.0",
    "stylelint": "^16.19.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^6.11.1",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.4"
  }
}
